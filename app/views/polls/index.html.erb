<h2>Polls Index</h2>

<!-- link to new poll -->
<%= link_to 'New Poll', new_poll_path(@poll) %>

<% @polls.each do |poll| %>
  <p>
    <div><%= poll.title %></div>
    <!-- links to show, edit, public url -->
    <%= link_to 'Show', admin_poll_path(poll.admin_url) %>
    <%= link_to 'Edit', edit_poll_path(poll) %>
    <%= link_to 'Public URL', public_poll_path(poll.public_url) %>
  </p>
<% end %>

